{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends":  [ 
        "eslint:recommended",
        "plugin:react/recommended",
        "next/core-web-vitals"
    ],
    "globals":{
        "process": "readonly"
 } ,
    
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": 0
    }
}
